

<MudDialog IsVisible="IsVisible" Class="backdrop-blur">
    <TitleContent>
        <MudText Typo="Typo.h6">
            @TitleContent
            <MudIcon Icon="@Icons.Material.Filled.Dangerous"></MudIcon>
        </MudText>
    </TitleContent>
    <DialogContent>
        @Content
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel" Color="Color.Error">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="Confirm">Confirm</MudButton>
    </DialogActions>
</MudDialog>


